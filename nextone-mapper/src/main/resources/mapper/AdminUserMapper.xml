<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nextone.mapper.AdminUserMapper">

    <select id="queryAll" resultType="com.nextone.pojo.AdminUser">
    select * from t_adminusers
    </select>
    <select id="findByUserName" parameterType="String" resultType="com.nextone.pojo.AdminUser">
        select * from t_adminusers
        where username =
        #{username}
    </select>

</mapper>